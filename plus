import sys

def tonum(s):
    try:
        return int(s)
    except ValusError:
        try:
            return float(s)
        except ValueError:
            print(f"Error: Invalid input '{s}'")
            sys.exit(1)

ans = 0
count = 0

for line in sys.stdin:
    line = line.rstrip()
    ans += tonum(line)
    count += 1

print("合計:", ans)
print("入力された数値の個数:", count)
